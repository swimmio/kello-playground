       Dialog CASE(OFF)
           Event CR
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # CR
           Event TAB
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # TAB
           Procedure OPCAO
               VALIDATE PRINCIPAL ;
               CLEAR-OBJECT LB1 ;
               CLEAR-OBJECT LB2 ;
               CLEAR-OBJECT LB3 ;
               CLEAR-OBJECT LB4 ;
               CLEAR-OBJECT LB5 ;
               CLEAR-OBJECT LB11 ;
               CLEAR-OBJECT LB6 ;
               SET-FLAG CARREGAR-DADOS(1) ;
               RETC ;
               REFRESH-OBJECT PRINCIPAL ;
               SET-FOCUS TABP1 ;
           End Procedure # OPCAO
           Event VAL-ERROR
               SET-FOCUS $EVENT-DATA ;
               INVOKE-MESSAGE-BOX MBOX-ERROS ERR-MSG $EVENT-DATA ;
               RETC ;
           End Event # VAL-ERROR
       End Dialog
   End Object  #EF53

