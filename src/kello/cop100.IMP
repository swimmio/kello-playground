   Object EF1
       Type ENTRY-FIELD
       Parent PRINCIPAL
       Stylename {font-003}
       Start (40,120)
       Size  (176,52)
       Masterfield CONTRATO
       Style AUTOSCROLL BORDER BLANK-WHEN-ZERO FIT-PICTURE RESIZE-CHAR('W')
       Style COLOR("BLUE","")
       Picture ZZZZ
       Dialog CASE(OFF)
           Event CR
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # CR
           Event TAB
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # TAB
           Procedure OPCAO
               MOVE CONTRATO CONTRATOW ;
               CLEAR-OBJECT PRINCIPAL ;
               CLEAR-OBJECT TAB1 ;
               MOVE CONTRATOW CONTRATO ;
               MOVE 3 TAXA ;
               REFRESH-OBJECT PRINCIPAL ;
               REFRESH-OBJECT TAB1 ;
               SKIP-TO-NEXT-OBJECT 1 ;
           End Procedure # OPCAO
           Event F2
               SET-FLAG POPUP-CONTRATO(1) ;
               RETC ;
               REFRESH-OBJECT EF1 ;
               MOVE 3 TAXA ;
               REFRESH-OBJECT EF53 ;
               SKIP-TO-NEXT-OBJECT 1 ;
           End Event # F2
       End Dialog
   End Object  #EF1

   Object EF53
       Type ENTRY-FIELD
       Parent PRINCIPAL
       Stylename {font-003}
       Start (252,120)
