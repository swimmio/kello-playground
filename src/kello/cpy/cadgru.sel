           SELECT CADGRU ASSIGN       TO   PATH-CADGRU
                         ORGANIZATION IS       INDEXED
                         ACCESS MODE  IS       DYNAMIC
                         STATUS       IS     ST-CADGRU
                         LOCK MODE    IS     AUTOMATIC
                         WITH LOCK    ON        RECORD
                         RECORD KEY   IS CADGRU-CODIGO
                         ALTERNATE    RECORD       KEY
                         IS                CADGRU-NOME.
